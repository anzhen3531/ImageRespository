VERSION 120

EDIT UG_GATEWAY_MAIN_MENUBAR

MENU WT_RMB_CTX_MENU
    
    BUTTON WT_RMB_CHECKIN
    LABEL 检入
    MESSAGE 检入选定文档。
    SENSITIVITY ON
    ACTIONS wt_rmb_checkin

    BUTTON WT_RMB_CHECKOUT
    LABEL 检出
    MESSAGE 检出选定文档。
    SENSITIVITY ON
    ACTIONS wt_rmb_checkout

    BUTTON WT_RMB_UPDATE
    LABEL 更新
    MESSAGE 更新选定文档。
    SENSITIVITY ON
    ACTIONS wt_rmb_update

    BUTTON WT_RMB_LOCK
    LABEL 锁定
    MESSAGE 锁定选定文档。
    SENSITIVITY ON
    ACTIONS wt_rmb_lock

    BUTTON WT_RMB_UNLOCK
    LABEL 解锁
    MESSAGE 解锁选定文档。
    SENSITIVITY ON
    ACTIONS wt_rmb_unlock

    BUTTON WT_RMB_UWGM_PROPERTIES
    LABEL 查看信息
    MESSAGE 启动文档的信息详细页面。
    SENSITIVITY ON
    ACTIONS wt_rmb_uwgm_properties
   
END_OF_MENU
HIDE WT_RMB_CTX_MENU

MENU WT_CHECKIN
   
   BUTTON WT_CHECKIN_CUSTOM
   LABEL 自定义检入
   MESSAGE 交互式检入活动文档。
   SENSITIVITY OFF
   ACTIONS wt_checkin_custom
   
   BUTTON WT_CHECKIN_AUTO
   LABEL 自动检入
   MESSAGE 使用操作的默认值检入文档。
   SENSITIVITY OFF
   ACTIONS wt_checkin_auto
   
END_OF_MENU

MENU WT_UPDATE

   BUTTON WT_UPDATE_CURRENT
   LABEL 更新活动模型
   MESSAGE 更新 Windchill 中包含信息的活动文档。
   SENSITIVITY OFF
   ACTIONS wt_update_current
   
   BUTTON WT_UPDATE_IN_SESSION
   LABEL 更新所有模型
   MESSAGE 使用 Windchill 中的信息更新所有打开的文档。
   SENSITIVITY OFF
   ACTIONS wt_update_in_session

END_OF_MENU
   
